{
    "name": "newfold-labs/wp-module-customer-bluehost",
    "description": "Customer module to interface with bluehost and the data module.",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://newfold-labs.github.io/satis/",
            "only": [
                "newfold-labs/*"
            ]
        },
        {
            "type": "composer",
            "url": "https://bluehost.github.io/satis/",
            "only": [
                "bluehost/*"
            ]
        }
    ],
    "authors": [
        {
            "name": "Evan Mullins",
            "email": "evan@bluehost.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NewFoldLabs\\WP\\Module\\CustomerBluehost\\": "includes"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "require": {
        "newfold-labs/wp-module-data": "^2.0.0"
    }
}